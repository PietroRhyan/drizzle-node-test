{
  "name": "node-test",
  "version": "1.0.0",
  "description": "Just a study backend with node",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "dev": "tsx watch --env-file .env src/index.ts",
    "studio": "drizzle-kit studio --port 3636 --verbose",
    "migrations:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "migrations:push": "drizzle-kit push:pg --config=drizzle.config.ts",
    "migrations:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "build": "tsx"
  },
  "author": "Pietro Rhyan",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "drizzle-kit": "^0.20.13",
    "pg": "^8.11.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.2",
    "express": "^4.18.2",
    "postgres": "^3.4.3",
    "vitest-tsconfig-paths": "^3.4.1",
    "zod": "^3.22.4"
  }
